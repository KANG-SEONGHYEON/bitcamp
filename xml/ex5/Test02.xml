<?xml version="1.0" encoding="UTF-8"?>
<!-- 기본 네임스페이스
- 태그를 그룹으로 묶을 때 사용하는 문법이다.
- 태그 관리를 쉽게하기 위해 사용한다.
네임스페이스 정의 문법
  <태그명 xmlns:별명="네임스페이스명">
- 네임스페이스명은 보통 URL로 표현한다.

네임스페이스 사용 문법
- 태그명
- 속성명
-->
<books xmlns="httpL//bitcamp.co.kr/java100/book"
       xmlns:a="httpL//bitcamp.co.kr/java100/author"
       xmlns:p="httpL//bitcamp.co.kr/java100/press">

    <a:author a:id="a001" a:name="홍길동"/>
    <a:author a:id="a002" a:name="임꺽정"/>
    <a:author a:id="a003" a:name="유관순"/>
    
    <p:press p:id="p001" name="비트출판사"/>
    <p:press p:id="p002" name="위키북스"/>
    <p:press p:id="p003" name="프리렉"/>
    
    <book title="aaaa" press="p001" author="a003"/>
</books>
